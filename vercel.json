{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "scrapers/daycycle.js", "use": "@vercel/node" },
    { "src": "scrapers/foodmenu.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/data", "dest": "/api/data.js" },
    { "src": "/api/daycycle", "dest": "/api/daycycle.js" },
    { "src": "/api/foodmenu", "dest": "/api/foodmenu.js" },
    {
      "src": "/api/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET,POST,OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Accept, api-key"
      }
    }
  ]
}
