{
    "version": 2,
    "builds": [
      { "src": "api/**/*.js", "use": "@vercel/node" },
      { "src": "scrapers/daycycle.py", "use": "@vercel/python", "config": { "runtime": "python3.9" } },
      { "src": "scrapers/foodmenu.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/data", "dest": "/api/supabase-data.js" },
      { "src": "/api/foodmenu", "dest": "/api/supabase-foodmenu.js" }
    ],
    "crons": [
      { "path": "/scrapers/daycycle.py", "schedule": "0 8 * * *" },
      { "path": "/scrapers/foodmenu.js", "schedule": "0 8 * * *" }
    ]
  }