{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "scrapers/daycycle.py", "use": "@vercel/python", "config": { "runtime": "python3.11" } },
    { "src": "scrapers/foodmenu.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/data", "dest": "/api/data.js" },
    { "src": "/api/foodmenu", "dest": "/api/foodmenu.js" },
    { "src": "/api/daycycle", "dest": "/api/daycycle.js" }
  ],
  "crons": [
    { "path": "/scrapers/daycycle.py", "schedule": "30 8 * * *" },
    { "path": "/scrapers/foodmenu.js", "schedule": "30 8 * * *" }
  ]
}