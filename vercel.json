{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "scrapers/daycycle.js", "use": "@vercel/node" },
    { "src": "scrapers/foodmenu.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/data", "dest": "/api/data.js" },
    { "src": "/api/foodmenu", "dest": "/api/foodmenu.js" }
  ],
  "crons": [
    { "path": "/scrapers/daycycle.js", "schedule": "09 4 * * *" },
    { "path": "/scrapers/foodmenu.js", "schedule": "09 4 * * *" }
  ]
}
